/*常规列表*/
.defaultList{
	background: white;
	margin: 0 10px;
}
.defaultList .icon{
	width:16px;
}
.defaultList .icon image{
	width: 100%;
}
.defaultList .text{
	padding-left: 10px;
	color: gray;
}
.defaultList .list{
	padding: 20px 10px;
	position: relative;
	font-size: 13px;
}
.defaultList .list:after{
	content: '';
